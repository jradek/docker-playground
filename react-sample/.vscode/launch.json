{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug with Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/src",
      "sourceMapPathOverrides": {
        "/workspaces/docker-playground/react-sample/src/*": "${webRoot}/*"
      },
      "runtimeArgs": ["--remote-debugging-port=9222"]
    }
  ]
}
